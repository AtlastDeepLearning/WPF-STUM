<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Background Colors -->
    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#eff2f7"/>
    <SolidColorBrush x:Key="SecundaryBackgroundColor" Color="#623ed0"/>
    <SolidColorBrush x:Key="Minimize" Color="#ffff00"/>
    <SolidColorBrush x:Key="Maximize" Color="#00b33c"/>

    <!-- Primary Colors -->
    <SolidColorBrush x:Key="PrimaryBlueColor" Color="#3772FF"/>

    <!-- Typograhy Colors -->
    <SolidColorBrush x:Key="PrimaryTextColor" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="TextSecundaryColor" Color="#B0B7C3"/>
    <SolidColorBrush x:Key="TextTertiaryColor" Color="#8A94A6"/>
    <SolidColorBrush x:Key="TextFourthColor" Color="#4B5D78"/>

    <!-- Icon Colors -->
    <SolidColorBrush x:Key="PrimaryIconColor" Color="#eff2f7"/>
    <SolidColorBrush x:Key="SecundaryIconColor" Color="#eff2f7"/>
    <SolidColorBrush x:Key="TertiarIconyColor" Color="#eff2f7"/>
    <SolidColorBrush x:Key="FourthIconColor" Color="#eff2f7"/>

    <SolidColorBrush x:Key="btnClose" Color="#F72626"/>

</ResourceDictionary>